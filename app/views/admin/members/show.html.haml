%h1= @member.name

%p
  %strong Email:
  = @member.email

%p
  %strong Member Id:
  = @member.customer_id

%p
  %strong Coupons:
  = @member.coupons.map(&:name).join(", ")

= link_to "Edit", edit_admin_member_path(@member), class: "btn btn-warning"
= link_to "Back", admin_members_path, class: "btn btn-secondary"
